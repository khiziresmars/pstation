{
    "name": "phuket/yacht-tours-api",
    "description": "Phuket Yacht & Tours API Backend",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2",
        "vlucas/phpdotenv": "^5.6",
        "bramus/router": "^1.6",
        "firebase/php-jwt": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true
    }
}
